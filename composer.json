{
    "name": "eporner/epornerlib",
    "description": "Official Eporner API v2 PHP client library",
    "keywords": ["eporner", "api", "porn", "video", "adult"],
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "EpornerLib\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EpornerLib\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.0",
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "suggest": {
        "ext-json": "Required for JSON response parsing"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "phpunit",
        "lint": "phpstan analyse src --level=max",
        "cs-check": "phpcs --standard=PSR12 src",
        "cs-fix": "phpcbf --standard=PSR12 src"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/eporner"
        }
    ]
}
